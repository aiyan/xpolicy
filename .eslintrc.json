{
  "env": {
    "commonjs": true,
    "es2020": true,
    "node": true,
    "jest/globals": true
  },
  "plugins": ["jest"],
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "no-empty": "off",
    "max-len": [
      "error",
      {
        "code": 80
      }
    ],
    "strict": "error"
  }
}
